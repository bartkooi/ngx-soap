{
  "name": "ngx-soap",
  "version": "0.14.0",
  "description": "SOAP service for Angular4",
  "main": "src/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "typings": "src/index.d.ts",
  "moduleName": "ngx-soap",
  "scripts": {
    "transpile": "ngc",
    "bundle": "rimraf dist && npm run transpile && rollup -c rollup.config.umd.js",
    "postbundle": "node tools/bundle-scripts/remove-code.js",
    "build": "npm run bundle && npm run copy",
    "copy": "node tools/bundle-scripts/copy-files.js && node tools/bundle-scripts/cleanup.js",
    "test": "rimraf spec/out && tsc -p tsconfig.spec.json && jasmine",
    "build:lib": "ng build --configuration production ngx-soap && copy README.md dist/ngx-soap && copy LICENSE dist/ngx-soap",
	"pack": "cd ./dist/ngx-soap/ && npm pack && cd ../.. && copy dist\\ngx-soap\\ngx*tgz ..",
    "postinstall": "ngcc"
  },
  "keywords": [
    "Angular",
    "Angular2",
    "Angular4",
    "Angular5",
    "Angular6",
    "Angular7",
    "@angular",
    "SOAP",
    "WSDL"
  ],
  "author": "Luca Lulani",
  "license": "MIT",
  "readme": "README.md",
  "homepage": "https://github.com/lula/ngx-soap",
  "bugs": {
    "url": "https://github.com/lula/ngx-soap/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lula/ngx-soap.git"
  },
  "dependencies": {
    "@angular/animations": "^14.2.1",
    "@angular/cdk": "^14.2.1",
    "@angular/common": "^14.2.1",
    "@angular/compiler": "^14.2.1",
    "@angular/core": "^14.2.1",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.2.1",
    "@angular/material": "^14.2.1",
    "@angular/platform-browser": "^14.2.1",
    "@angular/platform-browser-dynamic": "^14.2.1",
    "@angular/router": "^14.2.1",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "concat-stream": "^2.0.0",
    "core-js": "^3.25.1",
    "crypto-js": "^4.1.1",
    "events": "^3.0.0",
    "httpntlm": "^1.7.6",
    "lodash": "^4.17.10",
    "node": "^18.8.0",
    "rxjs": "^7.5.6",
    "sax": "^1.2.4",
    "stream": "0.0.2",
    "tslib": "^2.0.0",
    "url": "^0.11.0",
    "uuid": "^9.0.0",
    "xml-crypto": "^2.1.4",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.2",
    "@angular/cli": "^14.2.2",
    "@angular/compiler-cli": "^14.2.1",
    "@angular/language-service": "^14.2.1",
    "@types/events": "^3.0.0",
    "@types/jasmine": "^4.3.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^18.7.16",
    "codelyzer": "^0.0.28",
    "jasmine-core": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "ng-packagr": "^14.2.1",
    "protractor": "~7.0.0",
    "ts-node": "^10.9.1",
    "tslint": "~6.1.0",
    "typescript": "4.7"
  }
}
